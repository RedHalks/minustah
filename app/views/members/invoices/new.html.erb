<% content_for :title, t('.title') %>
<h3><%= t('.title') %></h3>

<%= form_with model: @invoice, url: member_invoices_path, method: :post do |form| %>
  <%= render partial: 'form', locals: { form: form } %>
  <%= form.submit t('helpers.submit.create', model: Invoice.model_name.human), :class  => "btn btn-md btn-primary" %>
<% end %>
